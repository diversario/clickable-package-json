{
  "manifest_version": 2,
  "name": "Linkify package.json",
  "short_name": "Linkify package.json",
  "description": "Turn dependencies into clickable npm links",
  "version": "1",
  "content_scripts": [
    {
      "matches": ["*://*/*blob*package.json"],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "*://*/*blob*package.json"
  ],
  "icons": {
    "128": "icon.png"
  }
}
