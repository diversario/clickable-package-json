{
  "manifest_version": 2,
  "name": "Linkify package.json",
  "short_name": "Linkify package.json",
  "description": "Turn dependencies into clickable npm or github links",
  "version": "2.5",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "jquery-2.1.4.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "webNavigation",
    "<all_urls>"
  ],
  "icons": {
    "128": "icon.png"
  }
}
